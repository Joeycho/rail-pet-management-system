<div class="topnav">
  <a href="/">Home</a>
  <a href="/login">Login</a>
  <a class="active" href="/owners/new">Sign Up</a>
</div>

<%@owner.errors.full_messages.each do |err|%>
  <li><%=err%></li>
  <%end%>

<h1>Owner Sign Up form</h1>
<%= form_for :owner do |f| %>
  Username: <%= f.text_field :name %>
  Email: <%= f.text_field :email %>
  Password: <%= f.password_field :password %>
  Password Confirmation: <%= f.password_field :password_confirmation %>
  <%= f.submit "Submit" %>
<% end %>
